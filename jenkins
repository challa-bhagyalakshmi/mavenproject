pipeline {
agent any
stages
{
stages
{
stage('Build')
{
steps {
echo "building the code"
bat "mvn clean"
}
}
stage('Test')
{
steps {
echo "testing the code"
bat "mvn test"
}
}
stage('compile')
{
steps {
echo "compiling the project"
bat 'mvn compile'
}
}
stage('deploy')
{
steps {
echo"deploying the project"
bat "mvn deploy"
}
}
}
}

